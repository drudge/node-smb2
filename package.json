  },
  "dependencies": {
    "des.js": "^1.1.0",
    "js-md4": "^0.3.2",
    "moment-timezone": "^0.5.31"
  },
  "devDependencies": {
    "@types/node": "^18.19.103",
    "nodemon": "^2.0.4",
    "ts-node": "^10.9.0",
    "tslint": "^6.1.0",
    "typescript": "^5.3.0"
  }
}
